{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.0.12",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.2"
  },
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start:js": "react-scripts start",
    "start": "npx npm-run-all -p watch:css start:js",
    "build:js": "react-scripts build",
    "build": "npx npm-run-all build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docker:dev": "docker-compose up -d --build",
    "docker:prod": "docker-compose -f docker-compose-p.yml up -d --build",
    "development": "npm run start",
    "production": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "google-maps": "^3.3.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-bootstrap": "^1.0.0-beta.9",
    "serve": "^11.0.2",
    "ts-node": "^8.3.0",
    "webpack": "^4.29.6"
  }
}
